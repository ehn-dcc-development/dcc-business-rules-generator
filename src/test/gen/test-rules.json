  "vaccination-before-today": {
    "Identifier": "VR-NL-0003",
    "Type": "Acceptance",
    "Country": "NL",
    "Version": "1.0.0",
    "SchemaVersion": "1.0.0",
    "Engine": "0.7.5",
    "CertificateType": "Vaccination",
    "Description": [
