  "vaccination-not-valid": {
    "Identifier": "VR-NL-0004",
    "Type": "Acceptance",
    "Country": "NL",
    "Version": "1.0.0",
    "SchemaVersion": "1.0.0",
    "Engine": "0.7.5",
    "CertificateType": "Vaccination",
    "Description": [
      {
        "lang": "en",
        "desc": "The delay time for this vaccine since this vaccination has not elapsed yet."
      }
    ],
    "ValidFrom": "2021-12-05T00:00:00+01:00",
    "ValidTo": "2030-06-01T00:00:00Z",
    "AffectedFields": [
      "v.0",
      "v.0.dn",
      "v.0.dt",
      "v.0.mp",
      "v.0.sd"
    ],
    "Logic": {
      "if": [
        {
          "var": "payload.v.0"
        },
        {
          "if": [
            {
              "===": [
                {
                  "var": "payload.v.0.mp"
                },
                "EU/1/20/1528"
              ]
            },
            {
              "if": [
                {
                  "and": [
                    {
                      "===": [
                        {
                          "var": "payload.v.0.dn"
                        },
                        1
                      ]
                    },
                    {
                      "===": [
                        {
                          "var": "payload.v.0.sd"
                        },
                        1
                      ]
                    }
                  ]
                },
                false,
                {
                  "if": [
                    {
                      "and": [
                        {
                          "===": [
                            {
                              "var": "payload.v.0.dn"
                            },
                            2
                          ]
                        },
                        {
                          "===": [
                            {
                              "var": "payload.v.0.sd"
                            },
                            2
                          ]
                        }
                      ]
                    },
                    {
                      "not-before": [
                        {
                          "plusTime": [
                            {
                              "var": "external.validationClock"
                            },
                            0,
                            "day"
                          ]
                        },
                        {
                          "plusTime": [
                            {
                              "var": "payload.v.0.dt"
                            },
                            14,
                            "day"
                          ]
                        }
                      ]
                    },
                    true
                  ]
                }
              ]
            },
            {
              "if": [
                {
                  "===": [
                    {
                      "var": "payload.v.0.mp"
                    },
                    "EU/1/20/1525"
                  ]
                },
                {
                  "if": [
                    {
                      "and": [
                        {
                          "===": [
                            {
                              "var": "payload.v.0.dn"
                            },
                            1
                          ]
                        },
                        {
                          "===": [
                            {
                              "var": "payload.v.0.sd"
                            },
                            1
                          ]
                        }
                      ]
                    },
                    {
                      "not-before": [
                        {
                          "plusTime": [
                            {
                              "var": "external.validationClock"
                            },
                            0,
                            "day"
                          ]
                        },
                        {
                          "plusTime": [
                            {
                              "var": "payload.v.0.dt"
                            },
                            28,
                            "day"
                          ]
                        }
                      ]
                    },
                    {
                      "if": [
                        {
                          "and": [
                            {
                              "===": [
                                {
                                  "var": "payload.v.0.dn"
                                },
                                3
                              ]
                            },
                            {
                              "===": [
                                {
                                  "var": "payload.v.0.sd"
                                },
                                2
                              ]
                            }
                          ]
                        },
                        false,
                        true
                      ]
                    }
                  ]
                },
                true
              ]
            }
          ]
        },
        true
      ]
    }
  },
